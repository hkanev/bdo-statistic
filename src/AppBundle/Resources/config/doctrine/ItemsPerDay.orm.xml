<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\ItemsPerDay" table="items_per_day">
    <indexes>
      <index name="items_per_day_fk" columns="item"/>
      <index name="items_per_day_fk_1" columns="date"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="quantity" type="integer" column="quantity" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="item" target-entity="Items" fetch="LAZY">
      <join-columns>
        <join-column name="item" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="date" target-entity="Dates" fetch="LAZY">
      <join-columns>
        <join-column name="date" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
